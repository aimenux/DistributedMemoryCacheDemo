{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Udp"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] [{SourceContext}] {Message:lj} {NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "rollingInterval": "Day",
          "path": "c:\\logs\\serilog-.log"
        }
      },
      {
        "Name": "Udp",
        "Args": {
          "remotePort": 7071,
          "family": "InterNetwork",
          "remoteAddress": "localhost",
          "formatter": "Serilog.Sinks.Udp.TextFormatters.Log4jTextFormatter, Serilog.Sinks.Udp"
        }
      }
    ]
  },
  "Settings": {
    "BlacklistUrls": [
      "https://twitter.com/",
      "https://www.snapchat.com/",
      "https://www.facebook.com/"
    ],
    "CacheSettings": {
      "ExpirationInMinutes": 2,
      "Source": "Memory",
      "Redis": {
        "ConnectionString": "localhost:6379"
      },
      "SqlServer": {
        "ConnectionString": "Server=localhost,1433;Database=CacheDatabase;User Id=sa;Password=@Pa5sw0rd!;TrustServerCertificate=True;",
        "SchemaName": "dbo",
        "TableName": "CacheTable"
      }
    }
  }
}